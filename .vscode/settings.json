{
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "eslint.nodePath": ".yarn/sdks",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.cjs",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "i18n-ally.localesPaths": ["messages", "src/i18n"],
  "i18n-ally.keystyle": "nested",
  "docwriter.style": "JSDoc",
  "docwriter.language": "English",
  "docwriter.progress.trackTypes": true,
  "docwriter.progress.trackClasses": true,
  "indentRainbow.indicatorStyle": "light",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#480011",
    "activityBar.background": "#480011",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#176000",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#480011",
    "statusBar.background": "#150005",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#480011",
    "statusBarItem.remoteBackground": "#150005",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#150005",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#15000599",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#150005"
}
