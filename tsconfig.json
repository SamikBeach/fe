{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@apis/*": ["legacy_src/apis/*"],
      "@app/*": ["legacy_src/app/*"],
      "@atoms/*": ["legacy_src/atoms/*"],
      "@elements/*": ["legacy_src/elements/*"],
      "@components/*": ["legacy_src/components/*"],
      "@constants/*": ["legacy_src/constants/*"],
      "@hooks/*": ["legacy_src/hooks/*"],
      "@models/*": ["legacy_src/models/*"],
      "@parsers/*": ["legacy_src/parsers/*"],
      "@svg/*": ["legacy_src/svg/*"],
      "@styles/*": ["legacy_src/styles/*"],
      "@utils/*": ["legacy_src/utils/*"],
      "@fonts/*": ["legacy_src/fonts/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
